{% extends 'base.html.twig' %}

{% block title %}{{notice.title}}{% endblock %}

{% block body %}
	{% if notice %}
	<div class="wrap-container">
		<div class="container">
			<div class="winco-food detail">
				<div class="title">
					<h2>{{notice.title}}</h2>
				</div>
				<div class="row">
					<div class="col-md-8">
						{{notice.content|raw}}
						<div class="blb d-none"></div>
					</div>
				</div>
			</div>
		</div>
	</div>
	{% endif %}
{% endblock %}